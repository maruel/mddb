{
  "name": "mddb-frontend",
  "version": "0.1.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "test": "vitest run",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "coverage": "vitest run --coverage",
    "typecheck": "tsc --noEmit",
    "lint": "tsc --noEmit && eslint frontend/src && prettier --check frontend/src && node frontend/scripts/check-css-modules.mjs",
    "lint:fix": "eslint frontend/src --fix && prettier --write frontend/src",
    "format": "prettier --write frontend/src"
  },
  "dependencies": {
    "@solid-primitives/i18n": "^2.2.1",
    "dompurify": "^3.3.1",
    "markdown-it": "^14.1.0",
    "prosemirror-commands": "^1.7.1",
    "prosemirror-dropcursor": "^1.8.2",
    "prosemirror-gapcursor": "^1.3.2",
    "prosemirror-history": "^1.4.1",
    "prosemirror-inputrules": "^1.5.0",
    "prosemirror-keymap": "^1.2.3",
    "prosemirror-markdown": "^1.13.2",
    "prosemirror-model": "^1.25.1",
    "prosemirror-schema-list": "^1.5.1",
    "prosemirror-state": "^1.4.3",
    "prosemirror-view": "^1.38.1",
    "solid-js": "^1.9.10",
    "terser": "^5.46.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.2",
    "@playwright/test": "^1.58.0",
    "@solidjs/testing-library": "^0.8.10",
    "@types/markdown-it": "^14.1.2",
    "@vitest/coverage-v8": "^4.0.18",
    "eslint": "^9.39.2",
    "eslint-plugin-solid": "^0.14.5",
    "globals": "^17.0.0",
    "jsdom": "^27.4.0",
    "prettier": "^3.8.1",
    "sharp": "^0.34.5",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.53.1",
    "vite": "^7.3.1",
    "vite-plugin-pwa": "^1.2.0",
    "vite-plugin-solid": "^2.11.10",
    "vitest": "^4.0.18"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "sharp"
    ],
    "overrides": {
      "seroval": "^1.4.2",
      "seroval-plugins": "^1.4.2"
    }
  }
}
