{
  "name": "mddb-frontend",
  "version": "0.1.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "test": "vitest run",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "coverage": "vitest run --coverage",
    "typecheck": "tsc --noEmit && tsc --noEmit -p tsconfig.e2e.json",
    "lint": "tsc --noEmit && tsc --noEmit -p tsconfig.e2e.json && eslint frontend/src sdk e2e playwright.config.ts && prettier --check frontend/src && node frontend/scripts/check-css-modules.mjs",
    "lint:fix": "eslint frontend/src sdk e2e playwright.config.ts --fix && prettier --write frontend/src",
    "format": "prettier --write frontend/src"
  },
  "dependencies": {
    "@solid-primitives/i18n": "^2.2.1",
    "@solidjs/router": "^0.15.4",
    "dompurify": "^3.3.1",
    "markdown-it": "^14.1.0",
    "prosemirror-commands": "^1.7.1",
    "prosemirror-dropcursor": "^1.8.2",
    "prosemirror-gapcursor": "^1.4.0",
    "prosemirror-history": "^1.5.0",
    "prosemirror-inputrules": "^1.5.1",
    "prosemirror-keymap": "^1.2.3",
    "prosemirror-markdown": "^1.13.4",
    "prosemirror-model": "^1.25.4",
    "prosemirror-schema-list": "^1.5.1",
    "prosemirror-state": "^1.4.4",
    "prosemirror-view": "^1.41.6",
    "solid-js": "^1.9.11",
    "terser": "^5.46.0"
  },
  "devDependencies": {
    "@eslint/js": "^10.0.1",
    "@material-symbols/svg-400": "^0.40.2",
    "@playwright/test": "^1.58.2",
    "@solidjs/testing-library": "^0.8.10",
    "@types/markdown-it": "^14.1.2",
    "@types/node": "^25.2.3",
    "@vitest/coverage-v8": "^4.0.18",
    "eslint": "^10.0.0",
    "eslint-plugin-solid": "^0.14.5",
    "globals": "^17.3.0",
    "jsdom": "^28.0.0",
    "prettier": "^3.8.1",
    "rollup-plugin-visualizer": "^6.0.5",
    "sharp": "^0.34.5",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.55.0",
    "vite": "^7.3.1",
    "vite-plugin-pwa": "^1.2.0",
    "vite-plugin-solid": "^2.11.10",
    "vite-solid-svg": "^0.0.4",
    "vitest": "^4.0.18",
    "workbox-precaching": "^7.4.0"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "sharp"
    ],
    "overrides": {
      "seroval": "^1.4.2",
      "seroval-plugins": "^1.4.2"
    }
  }
}
